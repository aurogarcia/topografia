{
  "name": "medidageo-website",
  "version": "1.0.0",
  "description": "Site profissional para MedidaGeo - Topografia e Georreferenciamento",
  "main": "index.html",
  "scripts": {
    "start": "live-server --port=3000",
    "build": "npm run minify-css && npm run minify-js",
    "minify-css": "cleancss -o css/styles.min.css css/styles.css",
    "minify-js": "uglifyjs js/script.js -o js/script.min.js",
    "dev": "live-server --port=3000 --watch=css,js",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "topografia",
    "website",
    "bootstrap",
    "responsive",
    "html5",
    "css3",
    "javascript",
    "surveying",
    "civil-engineering"
  ],
  "author": "MedidaGeo",
  "license": "MIT",
  "devDependencies": {
    "live-server": "^1.2.2",
    "clean-css-cli": "^5.6.2",
    "uglify-js": "^3.17.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}